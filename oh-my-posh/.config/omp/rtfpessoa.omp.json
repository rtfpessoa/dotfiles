{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{.PWD}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "overflow": "break",
      "segments": [
        {
          "properties": {
            "prefix": "",
            "postfix": ""
          },
          "template": "{{ if .SSHSession }}[ssh] {{ end }}",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "postfix": "",
            "prefix": ""
          },
          "template": "{{ if .SSHSession }}<#d75f00>{{ .UserName }}</> @ <#FFB86C>{{ .HostName }}</> in {{ end }}",
          "foreground": "#ffffff",
          "type": "session",
          "style": "plain",
          "foreground_templates": ["{{if .Root}}#ff5555{{end}}"]
        },
        {
          "properties": {
            "postfix": "",
            "prefix": "",
            "mixed_threshold": 12,
            "style": "mixed",
            "mapped_locations": {
              "~/go/src/github.com/DataDog": " üêï",
              "~/dd": " üêï",
              "~/Downloads": " üíæ",
              "~/dev": " üßë‚Äçüíª"
            }
          },
          "template": "\uf115  {{ .Path }} ",
          "foreground": "#50fa7b",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "postfix": "",
            "prefix": " ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }}",
          "foreground": "#bd93f9",
          "type": "git",
          "style": "plain"
        },
        {
          "properties": {
            "postfix": "",
            "prefix": " ",
            "cache_duration": "none"
          },
          "template": "{{if .Cluster}} \ufd31 {{.Cluster}} :: {{.Namespace}} {{end}}",
          "foreground": "#f1fa8c",
          "type": "kubectl",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "postfix": "",
            "prefix": " ",
            "time_format": "Mon, 02 Jan 2006 15:04:05 MST"
          },
          "template": "<#FFFFFF>\u23f1</>  {{ .CurrentDate | date .Format }}",
          "foreground": "#8be9fd",
          "type": "time",
          "style": "plain"
        },
        {
          "properties": {
            "style": "roundrock",
            "threshold": 500
          },
          "template": " <#fefefe>\ufbab</> {{ .FormattedMs }}",
          "foreground": "#8be9fd",
          "type": "executiontime",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "postfix": "",
            "prefix": " ",
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "<{{ if eq .Code 0 }}#ffffff{{ else }}#ff5555{{ end }}>{{ if .Root }}#{{ else }}\u21aa{{ end }}</> ",
          "foreground": "#ffffff",
          "type": "exit",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "tooltips": [
    {
      "properties": {
        "display_mode": "always",
        "home_enabled": true,
        "missing_command_text": "‚úï Go"
      },
      "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
      "foreground": "black",
      "background": "cyan",
      "type": "go",
      "style": "plain",
      "tips": ["go"]
    },
    {
      "properties": {
        "display_mode": "always",
        "home_enabled": true,
        "missing_command_text": "‚úï Java"
      },
      "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
      "foreground": "black",
      "background": "yellow",
      "type": "java",
      "style": "plain",
      "tips": ["java"]
    },
    {
      "properties": {
        "display_mode": "always",
        "home_enabled": true,
        "missing_command_text": "‚úï Node"
      },
      "template": " {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
      "foreground": "black",
      "background": "green",
      "type": "node",
      "style": "plain",
      "tips": ["node", "npm", "yarn"]
    },
    {
      "properties": {
        "display_mode": "always",
        "home_enabled": true,
        "missing_command_text": "‚úï Python"
      },
      "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
      "foreground": "black",
      "background": "blue",
      "type": "python",
      "style": "plain",
      "tips": ["python", "pip", "python2", "python3"]
    },
    {
      "properties": {
        "display_mode": "always",
        "home_enabled": true,
        "missing_command_text": "‚úï Ruby"
      },
      "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
      "foreground": "black",
      "background": "red",
      "type": "ruby",
      "style": "plain",
      "tips": ["ruby", "gem"]
    },
    {
      "properties": {
        "display_mode": "always",
        "home_enabled": true,
        "missing_command_text": "‚úï Rust"
      },
      "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
      "foreground": "black",
      "background": "magenta",
      "type": "rust",
      "style": "plain",
      "tips": ["rust"]
    },
    {
      "properties": {
        "parse_kubeconfig": true
      },
      "template": " {{ .Context }}{{ if .Namespace }} :: {{ .Namespace }}{{ end }} ",
      "foreground": "white",
      "background": "darkGray",
      "type": "kubectl",
      "style": "plain",
      "tips": ["kubectl", "k", "kubens", "kubectx"]
    }
  ],
  "version": 3
}
